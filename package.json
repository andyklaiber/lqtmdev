{
  "name": "lqtm",
  "version": "1.0.0",
  "description": "This project was bootstrapped with Fastify-CLI.",
  "main": "app.js",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "test": "tap \"test/**/*.test.js\"",
    "start": "fastify start -l info app.js",
    "dev": "fastify start -r dotenv/config -w --ignore-watch='newrelic_agent.log node_modules' -l info -P app.js",
    "build": "docker build . -t andyklaiber/lqtm:latest",
    "publish": "docker push andyklaiber/lqtm:latest",
    "docker": "docker run -p 3000:3000 --env UPLOAD_TOKEN=somethingsecret --mount source=pcrsdata,target=/usr/src/app/public/data andyklaiber/lqtm:latest"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@fastify/autoload": "^5.2.0",
    "@fastify/cors": "^8.1.0",
    "@fastify/mongodb": "^6.0.1",
    "@fastify/sensible": "^5.1.0",
    "@fastify/static": "^6.5.0",
    "boom": "^7.3.0",
    "dotenv": "^16.0.0",
    "fastify": "^4.4.0",
    "fastify-cli": "^5.0.1",
    "fastify-plugin": "^4.1.0",
    "fastify-websocket": "^4.2.1",
    "lodash": "^4.17.21",
    "moment": "^2.29.4",
    "mongoose": "^6.5.2",
    "stripe": "^10.1.0"
  },
  "devDependencies": {
    "tap": "^16.0.1"
  }
}
